{
  "name": "generative-art",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "vite",
    "host": "vite --host --port 3000",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:browser": "vitest run --config vitest.browser.config.ts",
    "test:browser:ui": "vitest --config vitest.browser.config.ts --ui",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:staged": "eslint --fix",
    "format": "prettier --write src/",
    "prepare": "husky",
    "webp": "node ./scripts/optimize.js",
    "docs:dev": "cd documentation && pnpm install --ignore-workspace && node node_modules/@docusaurus/core/bin/docusaurus.mjs start",
    "docs:build": "cd documentation && pnpm install --ignore-workspace && node node_modules/@docusaurus/core/bin/docusaurus.mjs build"
  },
  "lint-staged": {
    "*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@webgamekit/controls": "workspace:^",
    "@webgamekit/threejs": "workspace:^",
    "dat.gui": "^0.7.9",
    "p5": "^1.9.0",
    "pinia": "^2.3.0",
    "stats.js": "^0.17.0",
    "three": "^0.178.0",
    "vite-plugin-wasm": "^3.3.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@babel/types": "^7.23.9",
    "@dimforge/rapier3d-compat": "^0.19.3",
    "@eslint/js": "^9.39.2",
    "@rushstack/eslint-patch": "^1.3.3",
    "@sentry/vue": "^10.29.0",
    "@tsconfig/node20": "^20.1.2",
    "@types/dat.gui": "^0.7.12",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.28",
    "@types/p5": "^1.7.6",
    "@types/stats.js": "^0.17.3",
    "@types/three": "^0.179.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.24",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^9.1.7",
    "jsdom": "^24.0.0",
    "lint-staged": "^16.2.7",
    "lucide-vue-next": "^0.563.0",
    "npm-run-all2": "^6.1.1",
    "playwright": "^1.58.0",
    "postcss": "^8.5.6",
    "prettier": "^3.0.3",
    "radix-vue": "^1.9.17",
    "sharp": "^0.34.5",
    "typescript": "~5.4.0",
    "typescript-plugin-css-modules": "^5.2.0",
    "vite": "^5.1.6",
    "vitest": "^4.0.18",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^2.0.6"
  }
}
